# Font: http://editorconfig.org

root = true

[*]
indent_size = 2
indent_style = tab
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
end_of_line = crlf
max_line_length = 100

[*.rs]
indent_style = space

[*.py]
indent_size = 4
indent_style = space

[Makefile]
indent_size = 4
indent_style = tab

